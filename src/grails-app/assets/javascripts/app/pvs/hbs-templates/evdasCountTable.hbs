<table class="table table-striped table-curved table-hover" >
    <thead>
    <tr>
        <th>Date</th>
        <th>EV</th>
        <th>Lit</th>
        <th>Serious</th>
        <th>Fatal</th>
    </tr>
    </thead>
    {{#each trendData}}
        <tr>
            <td>{{xAxisTitle}}</td>
            <td>{{evCount}}</td>
            <td>{{litCount}}</td>
            <td>{{seriousCount}}</td>
            <td>{{fatalCount}}</td>
        </tr>
    {{/each}}
</table>
